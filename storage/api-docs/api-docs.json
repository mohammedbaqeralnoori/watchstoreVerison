            "post": {
                "tags": [
                    "Auth Api"
                ],
                "description": "Send verification sms",
                "operationId": "91b5be4ce0adfcf4f905dbf1b418370e",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "mobile": {
                                        "type": "string",
                                        "example": "09371778560"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "SMS sent"
                    },
                    "429": {
                        "description": "Please wait"
                    }
                }
            }
        },
